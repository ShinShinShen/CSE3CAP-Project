{
  "vendor_mappings": {
    "client 1 or 3 xlsx": {
      "detect_headers_any": ["seq #", "source", "destination", "service", "action"],
      "columns": {
        "id": ["seq #"],
        "name": ["name"],
        "srcaddr": ["source", "srcaddr"],
        "src_address": ["address"],
        "dstaddr": ["destination", "dstaddr"],
        "dst_address": ["address.1"],   
        "service": ["service"],
        "action": ["action"],
        "log": ["log"],
        "comment": ["comments"],
        "risk_rating": ["risk"]
      },
      "defaults": {
        "log": "log all sessions",
        "comment": ""
      }
    },

    "checkpoint": {
      "detect_headers_any": ["num", "source", "destination", "service", "action"],
      "columns": {
        "id": ["num"],
        "name": ["name"],
        "srcaddr": ["source"],
        "dstaddr": ["destination"],
        "service": ["service"],
        "action": ["action"],
        "log": ["track"],
        "comment": ["comments"],
        "status": ["enabled"],
        "install_on": ["install_on"]
      },
      "defaults": {
        "log": "log all sessions",
        "comment": "",
        "status": "enable"
      },
      "normalization": {
        "status": {
          "true": "enable",
          "false": "disable"
        }

      }
    },

    "sophos": {
  "detect_headers_any": ["id", "name", "source", "destination", "services", "action"],
  "columns": {
    "id": ["id"],
    "name": ["name"],
    "srcaddr": ["source"],
    "dstaddr": ["destination"],
    "service": ["services"],
    "action": ["action"],
    "log": ["log_traffic"],
    "comment": ["comment"],
    "status": ["status"]
  },
  "defaults": {
    "log": "enabled",
    "comment": "",
    "status": "enable"
  },
  "normalization": {
    "log": {
      "true": "enabled",
      "false": "disable"
    },
    "status": {
      "enabled": "enable",
      "disabled": "disable"
    }
  }
},

    "sdwan": {
      "detect_headers_any": ["rule id", "src-ip", "dst-ip", "protocol", "action"],
      "columns": {
        "id": ["rule id"],
        "name": ["name"],
        "srcaddr": ["src-ip"],
        "dstaddr": ["dst-ip"],
        "service": ["protocol"],
        "action": ["action"],
        "log": ["logging"],
        "comment": ["description"]
      },
      "defaults": {
        "log": "enabled",
        "comment": ""
      }
    },

    "client2": {
      "detect_headers_any": ["id", "uuid", "srcintf", "dstintf", "srcaddr", "dstaddr", "service", "action"],
      "columns": {
        "id": ["id"],
        "name": ["name"],
        "srcaddr": ["srcaddr"],
        "dstaddr": ["dstaddr"],
        "service": ["service"],
        "action": ["action"],
        "log": ["logtraffic"],
        "comment": ["comments", "tl comment"],
        "risk_rating": ["rating"]
      },
      "defaults": {
        "log": "all",
        "comment": ""
      }
    },

    "client3_csv": {
      "detect_headers_any": ["policyid", "srcaddr", "dstaddr", "service", "action"],
      "columns": {
        "id": ["policyid"],
        "name": ["comments"],
        "srcaddr": ["srcaddr"],
        "dstaddr": ["dstaddr"],
        "service": ["service"],
        "action": ["action"],
        "log": ["log"],
        "comment": ["comments"],
        "status": ["status"],
        "srcaddr_negate": ["srcaddr-negate"],
        "dstaddr_negate": ["dstaddr-negate"],
        "service_negate": ["service-negate"]
      },
      "defaults": {
        "log": "log all sessions",
        "comment": "",
        "status": "enable"
      },
      "risk_rules": {
        "allow_all_source_port_client3": {
          "enabled": true,
          "match": {
            "srcaddr": ["any", "all", "0.0.0.0/0"],
            "srcaddr_negate": ["disable"],
            "status": ["enable"],
            "action": ["accept", "allow"]
          },
          "severity": "HIGH",
          "category": "User Access"
        },
        "allow_all_destination_port_client3": {
          "enabled": true,
          "match": {
            "dstaddr": ["any", "all", "0.0.0.0/0"],
            "dstaddr_negate": ["disable"],
            "status": ["enable"],
            "action": ["accept", "allow"]
          },
          "severity": "HIGH",
          "category": "User Access"
        },
        "allow_all_service_client3": {
          "enabled": true,
          "match": {
            "service": ["all", "ip/0", "any"],
            "service_negate": ["disable"],
            "status": ["enable"],
            "action": ["accept", "allow"]
          },
          "severity": "HIGH",
          "category": "Web"
        },
        "service_admin_port_exposed_client3": {
          "enabled": true,
          "match_ports": {
            "service": ["ssh", "23", "3389", "5900", "5985", "5986"]
          },
          "match": {
            "service_negate": ["disable"],
            "status": ["enable"],
            "action": ["accept", "allow"]
          },
          "severity": "CRITICAL",
          "category": "Admin"
        },
        "smb_netbios_exposed_client3": {
          "enabled": true,
          "match_ports": {
            "service": ["137", "138", "139", "445"]
          },
          "match": {
            "service_negate": ["disable"],
            "status": ["enable"],
            "action": ["accept", "allow"]
          },
          "severity": "CRITICAL",
          "category": "Web"
        },
        "suspicious_service_client3": {
          "enabled": true,
          "match": {
            "service": ["telnet", "ftp", "tftp", "irc"],
            "service_negate": ["disable"],
            "status": ["enable"],
            "action": ["accept", "allow"]
          },
          "severity": "HIGH",
          "category": "Web"
        },
        "no_logging_client3": {
          "enabled": true,
          "match": {
            "log": ["disable", "no log", "none", "log violation traffic"],
            "status": ["enable"],
            "action": ["accept", "allow"]
          },
          "severity": "MEDIUM",
          "category": "Admin"
        }
      }
    }
  },

  "risk_rules": {
    "allow_all_source_port": {
      "enabled": true,
      "match": {
        "srcaddr": ["any", "all", "0.0.0.0/0"],
        "action": ["accept", "allow"]
      },
      "severity": "HIGH",
      "category": "User Access"
    },
    "allow_all_destination_port": {
      "enabled": true,
      "match": {
        "dstaddr": ["any", "all", "0.0.0.0/0"],
        "action": ["accept", "allow"]
      },
      "severity": "HIGH",
      "category": "User Access"
    },
    "allow_all_service": {
      "enabled": true,
      "match": {
        "service": ["all", "ip/0", "any"],
        "action": ["accept", "allow"]
      },
      "severity": "MEDIUM",
      "category": "Web"
    },

    "smb_netbios_exposed": {
      "enabled": true,
      "match_ports": { "service": ["137", "135", "138", "139", "445", "netbios", "'service-smb'"] },
      "action_scope": ["accept", "allow"],
      "severity": "CRITICAL",
      "category": "Web"
    },
  
    "admin_port_exposed": {
      "enabled": true,
      "match_ports": { "service": ["ssh", "23", "3389", "5900", "5985", "5986", "rdp", "22", "'service-ssh'","service-rdp"] },
      "action_scope": ["accept", "allow"],
      "severity": "CRITICAL",
      "category": "Admin"
    },
    "suspicious_service": {
      "enabled": true,
      "match": { "service": ["tftp", "irc", "smtp", "ldap", "25" ] },
      "action_scope": ["accept", "allow"],
      "severity": "High",
      "category": "Web"
    },
    "LPD_Port_Exposure": {
      "enabled": true,
      "match": { "service": ["lpdw0rm"] },
      "action_scope": ["accept", "allow"],
      "severity": "High",
      "category": "Web"
    },
    "broad_ip_range": {
      "enabled": true,
      "values": ["all", "0.0.0.0/0", "::/0"],
      "action_scope": ["accept", "allow"],
      "severity": "HIGH",
      "category": "User Access"
    },
    "no_logging_error": {
      "enabled": true,
      "match": { "log": ["disable", "false", "no log", "none", "log violation traffic"] },
      "action_scope": ["accept", "allow"],
      "severity": "MEDIUM",
      "category": "Admin"
    },
    "weak_protocol": {
      "enabled": true,
      "match": { "service": ["http", "ftp", "telnet", "21"] },
      "action_scope": ["accept", "allow"],
      "severity": "HIGH",
      "category": "Web"
    },
    "any_source_high_port": {
      "enabled": false,
      "match": {
        "srcaddr": ["any", "all", "0.0.0.0/0"],
        "dst_port": ["1024-65535"],
        "action": ["accept", "allow"]
      },
      "severity": "MEDIUM",
      "category": "User Access"
    }
  },

  "service_port_map": {
    "ssh": "ssh",
    "telnet": "23",
    "rdp": "rdp",
    "vnc": "5900",
    "winrm-http": "5985",
    "winrm-https": "5986",
    "nbt": "137",
    "netbios-dgm": "138",
    "tcp_135": "135",
    "microsoft-ds": "445",
    "[rdp]": "rdp",
    "grp_netbios": "netbios"





  }
}
